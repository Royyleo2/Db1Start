sexo = str(input('Qual o seu gÃªnero [M/F]: ')).strip().upper()[0]
while sexo not in 'MmFf':
    sexo = str(input('Dados incorretos, por favor, informe seu genero [M/F]: ')).strip().upper()[0]
if sexo in "Mn":
    print ('Genero Masculinho cadastrado')
else:
    print ('Genero Feminino cadastrado')

